{
  "manifest_version": 2,
  "name": "Copy",
  "version": "1.0",
  "description": "Copy url, etc.",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "Copy",
    "default_popup": "popup/popup.html"
  }
}
